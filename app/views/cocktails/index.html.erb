<div class="container">
  <div class="header">
  <h1> Welcome to The Mixology Room </h1>
  <h2> Find the perfect cocktail for you <h2>
  </div>

  <div class="cocktail-card">
    <% @cocktails.each do |cocktail| %>
      <img src=<%= cocktail.image_url%> alt= <%= cocktail.name %>>
      <h3><%= cocktail.name %></h3>
      <p><%= cocktail.alcoholic%></p>
    <%end%>
  </div>
</div>
